{
  "name": "commandsocket",
  "displayName": "CommandSocket",
  "description": "Allows to control VSCode via a WebSocket.",
  "version": "1.1.1",
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "CommandSocket",
      "properties": {
        "commandsocket.port": {
          "type": "number",
          "default": 6783,
          "description": "Port to use for WebSocket Server."
        },
        "commandsocket.password": {
          "type": "string",
          "default": "",
          "description": "Password to encrypt communication. Leave blank to disable encryption."
        },
        "commandsocket.host": {
          "type": "string",
          "default": "0.0.0.0",
          "description": "Host to bind WebSocket Server to."
        },
        "commandsocket.newCryptoWarning": {
          "type": "boolean",
          "default": true,
          "description": "Warn about new encryption method."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.1",
    "@types/node": "20.11.4",
    "@types/vscode": "^1.76.0",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "ws": "^8.12.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pascaldiehm/vscode-commandsocket"
  },
  "icon": "icon.png",
  "license": "MIT",
  "publisher": "pascaldiehm"
}
